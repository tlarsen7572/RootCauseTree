<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:com.PorcupineSupernova.RootCauseTreeWpf">
    <Color x:Key="LightBrown" R="245" G="222" B="179" A="255" />
    <Color x:Key="DarkBrown" R="214" G="186" B="133" A="255" />
    <Color x:Key="VeryLightGreen" R="240" G="255" B="240" A="255" />
    <Color x:Key="LightGreen" R="204" G="255" B="206" A="255" />
    <Color x:Key="DarkGreen" R="178" G="234" B="172" A="255" />
    <Color x:Key="ErrorMsgText" R="155" G="0" B="0" A="255" />
    <Color x:Key="BrownFont" R="134" G="103" B="45" A="255" />
    <Color x:Key="DisabledFont" R="80" G="80" B="80" A="255" />
    <FontFamily x:Key="DefaultFontFamily">Segoe UI Symbol</FontFamily>
    <Style x:Key="IconFont" TargetType="Control">
        <Setter Property="FontFamily" Value="{StaticResource DefaultFontFamily}"/>
        <Setter Property="FontSize" Value="14 pt"/>
    </Style>
    <Style x:Key="DefaultFont" TargetType="Control">
        <Setter Property="FontFamily" Value="{StaticResource DefaultFontFamily}"/>
        <Setter Property="FontSize" Value="11 pt"/>
    </Style>
    <Style x:Key="ErrorFont" TargetType="Control">
        <Setter Property="FontFamily" Value="{StaticResource DefaultFontFamily}"/>
        <Setter Property="FontSize" Value="9 pt"/>
    </Style>
</ResourceDictionary>